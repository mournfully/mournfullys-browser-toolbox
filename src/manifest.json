{
  "name": "Mournfully's Extension",
  "version": "1.2.2",
  "manifest_version": 3,
  "description": "An amalgamation of a couple distinct chrome extensions under one unified interface",
  "homepage_url": "https://github.com/mournfully",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "clipboardWrite"
  ]
}
